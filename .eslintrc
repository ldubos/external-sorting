{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2019,
    "sourceType": "module"
  },
  "extends": [
    "airbnb-base",
    "plugin:@typescript-eslint/recommended",
  ],
  "env": {
    "node": true,
    "es6": true
  },
  "rules": {
    "comma-dangle": 0,
    "no-plusplus": 0,
    "no-cond-assign": 0,
    "no-continue": 0,
    "@typescript-eslint/no-explicit-any": 0
  }
}
